<p style="color: green"><%= notice %></p>

<% content_for :title, "Atendimentos" %>

<h1>Atendimentos</h1>

<div id="atendimentos">
  <% @atendimentos.each do |atendimento| %>
    <%= render atendimento %>
    <p>
      <%= link_to "Ver este atendimento", atendimento %>
    </p>
  <% end %>
</div>

<%= link_to "Novo atendimento", new_atendimento_path %>
